{	"setup":{
		"autopilot": {},
		"gazebo": {
			"gazebo_models": {
				"gazebo_model_repo_0":{
					"repo": "git@github.com:rudislabs/nxp_gazebo.git",
					"version": "aim",
					"name": "nxp_gazebo"
				},
				"gazebo_model_repo_1":{
					"repo": "git@github.com:osrf/gazebo_models.git",
					"version": "master",
					"name": "osrf/models"
				}
			},
			"gazebo_plugins": {}
		},
		"ros2": {
			"ros2_repo_0":{
				"repo": "git@github.com:rudislabs/nxp_cup_interfaces.git",
				"version": "aim",
				"build_package": "nxp_cup_interfaces",
				"build_prefix": "--packages-select",
				"build_postfix": "--symlink-install"
			},
			"ros2_repo_1":{
				"repo": "git@github.com:rudislabs/nxp_cup_vision.git",
				"version": "aim",
				"build_package": "nxp_cup_vision",
				"build_prefix": "--packages-select",
				"build_postfix": "--symlink-install"
			},
			"ros2_repo_2":{
				"repo": "git@github.com:rudislabs/aim_line_follow.git",
				"version": "main",
				"build_package": "aim_line_follow",
				"build_prefix": "--packages-select",
				"build_postfix": "--symlink-install"
			}
		},
		"system": {
			"set_environment": {
				"environment_0": {
					"variable": "GAZEBO_MODEL_DATABASE_URI",
					"value": "",
					"method": "overwrite"
				},
				"environment_1": {
					"variable": "GAZEBO_RESOURCE_PATH",
					"value": "/usr/share/gazebo-11",
					"method": "prepend"
				},
				"environment_2": {
					"variable": "GAZEBO_PLUGIN_PATH",
					"value": "/opt/ros/foxy/lib",
					"method": "prepend"
				}
			}
		}
	},
	"verbose": {
		"gzclient": true,
		"gzserver": true
	},
	"nodes": {
		"ros2_node_0":{
			"timing": "post-spawn",
			"package": "nxp_cup_vision",
			"executable": "nxp_track_vision",
			"name": "nxp_track_vision_0",
			"output": "screen",
			"parameters": [	{"pyramid_down": 3},
							{"camera_image": "trackImage0"},
							{"debug_image": "debugImage0"},
							{"namespace": "cupcar0"},
							{"mask_ratio_array": [1.0, 0.4]}

						]
		},
		"ros2_node_1":{
			"timing": "post-spawn",
			"package": "rqt_image_view",
			"executable": "rqt_image_view",
			"name": "rqt_image_view_0",
			"output": "screen",
			"parameters": []
		},
		"ros2_node_2":{
			"timing": "post-spawn",
			"package": "aim_line_follow",
			"executable": "line_follower",
			"name": "line_follower_0",
			"output": "screen",
			"parameters": [	{"start_delay": 5.0},
							{"camera_vector_topic": "/cupcar0/PixyVector"},
							{"linear_velocity": 2.0},
							{"angular_velocity": -0.8},
							{"single_line_steer_scale": 0.6}
						]
		}
	},
	"world_params": {
		"gazebo_name": "nxp_gazebo",
		"generate_world": 1,
		"generate_params":{
			"world_name": "canvas",
			"sdf_version": "1.6",
			"use_spherical_coords": 1,
			"lat_lon_alt": [28.6139, -77.2090, 100],
			"sun_model": "sunUTC",
			"sun_utc_date": "now",
			"ambient_light": 0.5,
			"background_light": 0.15,
			"shadows": 0,
			"cloud_speed": 12,
			"wind_speed": "NotSet",
			"embedded_models": {
				"embed_model_0": {
					"model": "nxp_raceway_overpass",
					"name": "nxp_raceway_overpass_0",
					"pose": "0 0 -0.009 0 0 0"
				},
				"embed_model_1": {
					"model": "start_point",
					"name": "start_point_0",
					"pose": "0.2 -0.45 0.09 0 0 0"
				},
				"embed_model_2": {
					"model": "start_point",
					"name": "start_point_1",
					"pose": "0.2 0.45 0.09 0 0 0"
				}
			},
			"video_widget": "NotSet",
			"set_physics": 0,
			"update_rate": 100,
			"realtime_factor": 1.0,
			"ode_threads": 2
		}
	},
	"models": {
		"model_params_0": {
			"gazebo_name": "nxp_gazebo",
			"spawn_pose": [0, 0, 0.1, 0, 0, 0],
			"instance": 0,
			"generate_params":{
				"controller": "ros2",
				"base_model": "nxp_cupcar",
				"sdf_version": 1.6,
				"camera_image": "trackImage0",
				"namespace": "cupcar0",
				"model_name": "NotSet",
				"hq_wheel": "0",
				"hq_frame": "0"
			}
		}
	}
}
